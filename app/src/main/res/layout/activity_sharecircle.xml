<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="ca.imdc.newp.shareCircleActivity"
    android:id="@+id/sharecircle">


    <android.support.design.widget.FloatingActionButton
        android:id="@+id/contact1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:clickable="true"
        app:backgroundTint="@android:color/transparent"
        app:srcCompat="@android:drawable/ic_input_add" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/contact2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/contact1"
        android:layout_marginEnd="49dp"
        android:layout_marginTop="51dp"
        android:layout_toStartOf="@+id/contact1"
        android:clickable="true"
        app:backgroundTint="@android:color/transparent"
        app:srcCompat="@android:drawable/ic_input_add" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/contact3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/contact2"
        android:layout_toEndOf="@+id/contact2"
        android:clickable="true"
        app:backgroundTint="@android:color/transparent"
        app:srcCompat="@android:drawable/ic_input_add" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/contact4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/contact2"
        android:layout_centerVertical="true"
        android:clickable="true"
        app:backgroundTint="@android:color/transparent"
        app:srcCompat="@android:drawable/ic_input_add" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/contact5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_above="@+id/contact4"
        android:layout_alignStart="@+id/contact4"
        android:layout_marginBottom="54dp"
        android:clickable="true"
        app:backgroundTint="@android:color/transparent"
        app:srcCompat="@android:drawable/ic_input_add" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/contact6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/contact5"
        android:layout_alignStart="@+id/contact1"
        android:clickable="true"
        app:backgroundTint="@android:color/transparent"
        app:srcCompat="@android:drawable/ic_input_add" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/contact7"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/contact6"
        android:layout_marginStart="56dp"
        android:layout_toEndOf="@+id/contact6"
        android:clickable="true"
        app:backgroundTint="@android:color/transparent"
        app:srcCompat="@android:drawable/ic_input_add" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/contact8"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/contact1"
        android:layout_alignStart="@+id/contact7"
        android:clickable="true"
        app:backgroundTint="@android:color/transparent"
        app:srcCompat="@android:drawable/ic_input_add" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/contact9"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/contact8"
        android:layout_alignTop="@+id/contact3"
        android:clickable="true"
        android:visibility="visible"
        app:backgroundTint="@android:color/transparent"
        app:srcCompat="@android:drawable/ic_input_add" />
</RelativeLayout>